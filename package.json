    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "8.1.5",
    "mockdate": "2.0.2",
    "prettier": "1.16.4",
    "strip-color": "0.1.0"
