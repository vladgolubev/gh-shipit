{
  "name": "gh-shipit",
  "version": "1.11.3",
  "description": "Ship Releases",
  "keywords": [
    "github",
    "organization",
    "release"
  ],
  "repository": "vladgolubev/gh-shipit",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "golubev.vld@gmail.com",
    "url": "vladholubiev.com"
  },
  "files": [
    "lib/"
  ],
  "bin": {
    "shipit": "./lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib/ && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts'",
    "coverage": "TZ=UTC jest --coverage",
    "lint": "eslint . --ext .js,.ts,.json --fix",
    "lint:ci": "eslint . --ext .js,.ts,.json",
    "prepack": "yarn build",
    "start": "node ./lib/cli.js",
    "start:approve-server": "node ./lib/approve-server.js",
    "test": "TZ=UTC FORCE_COLOR=0 jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,json,md,yml,graphql}": [
      "prettier --write --ignore-path=./.eslintignore",
      "git add"
    ],
    "*.{ts,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@octokit/graphql-schema": "4.27.0",
    "@octokit/rest": "16.36.0",
    "@shelf/gh-sdk": "0.2.3",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-context": "1.0.19",
    "apollo-link-http": "1.5.16",
    "boxen": "4.2.0",
    "chalk": "2.4.2",
    "cli-table3": "0.5.1",
    "cli-width": "2.2.0",
    "date-fns": "1.30.1",
    "debug": "4.1.1",
    "enquirer": "2.3.6",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "human-date": "1.4.0",
    "inquirer": "7.3.2",
    "json2csv": "4.5.4",
    "lodash": "4.17.15",
    "log-symbols": "3.0.0",
    "longest": "2.0.1",
    "node-fetch": "2.6.0",
    "normalize-space-x": "3.0.0",
    "opn": "5.5.0",
    "ora": "3.4.0",
    "p-map": "3.0.0",
    "p-progress": "0.4.1",
    "pretty-version-diff": "1.0.0",
    "progress": "2.0.3",
    "semver": "6.3.0",
    "smartwrap": "1.1.2",
    "string-width": "4.2.0",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@shelf/eslint-config": "0.23.1",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.2",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.1.2",
    "@types/json2csv": "4.5.1",
    "@types/lodash": "4.14.149",
    "@types/mockdate": "2.0.0",
    "@types/node-fetch": "2.5.4",
    "@types/progress": "2.0.3",
    "@types/semver": "7.1.0",
    "@types/strip-color": "0.1.0",
    "@types/update-notifier": "2.5.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "jest": "25.2.4",
    "lint-staged": "10.0.10",
    "mockdate": "2.0.5",
    "prettier": "1.19.1",
    "strip-color": "0.1.0",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=12"
  }
}
