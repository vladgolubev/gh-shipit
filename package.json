{
  "name": "gh-shipit",
  "version": "1.8.2",
  "description": "Ship Releases",
  "keywords": [
    "github",
    "organization",
    "release"
  ],
  "repository": "vladgolubev/gh-shipit",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "golubev.vld@gmail.com",
    "url": "vladholubiev.com"
  },
  "files": [
    "src/"
  ],
  "bin": {
    "shipit": "./src/cli.js"
  },
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint . --fix --ext .js,.json,.ts --quiet",
    "start": "node ./src/cli.js",
    "test": "FORCE_COLOR=0 jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml,graphql}": [
      "prettier --write --ignore-path=./.eslintignore",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@octokit/graphql-schema": "4.27.0",
    "@octokit/rest": "15.18.2",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-context": "1.0.19",
    "apollo-link-http": "1.5.16",
    "boxen": "4.1.0",
    "chalk": "2.4.2",
    "cli-table3": "0.5.1",
    "cli-width": "2.2.0",
    "date-fns": "1.30.1",
    "debug": "4.1.1",
    "graphql": "14.3.1",
    "graphql-tag": "2.10.1",
    "human-date": "1.4.0",
    "inquirer": "6.4.1",
    "json2csv": "4.5.1",
    "lodash": "4.17.11",
    "log-symbols": "3.0.0",
    "longest": "2.0.1",
    "node-fetch": "2.6.0",
    "normalize-space-x": "3.0.0",
    "opn": "5.5.0",
    "ora": "3.4.0",
    "p-map": "3.0.0",
    "p-progress": "0.4.1",
    "pretty-version-diff": "1.0.0",
    "progress": "2.0.3",
    "semver": "6.1.2",
    "smartwrap": "1.0.12",
    "string-width": "4.1.0",
    "update-notifier": "3.0.0"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.6.0",
    "eslint": "6.4.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "mockdate": "2.0.3",
    "prettier": "1.18.2",
    "strip-color": "0.1.0"
  },
  "engines": {
    "node": ">=8"
  }
}
