    "cli-width": "2.2.0",
    "date-fns": "1.30.1",
    "debug": "4.1.1",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "human-date": "1.4.0",
    "inquirer": "6.2.2",
    "json2csv": "4.4.1",
